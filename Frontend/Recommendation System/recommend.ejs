	<h1> TAGS </h1>
	<div class="tags">

		<div class="box1">
			<h4>Weak</h4>

			 <% if(weakarea.length > 0){ %>

            	<% for (var i = 0 ; i < weakarea.length-1 ; i++){ %>

                    <a href="#" class="btn btn-danger" title="">
                        <span class="text-sm text-gray">
                            <%= weakarea[i]%>
                         </span>
                     </a>
              <% } %>
              <% } %>

		</div>


		<div class="box2">
			<h4>Good</h4>

			<% if(goodarea.length > 0){ %>

                        	<% for (var i = 0 ; i < goodarea.length-1 ; i++){ %>

                               <a href="#" class="btn btn-warning" title="">
                                    <span class="text-sm text-gray">
                                        <%= goodarea[i]%>
                                     </span>
                                 </a>
                          <% } %>
                          <% } %>
		</div>


		<div class="box3">
			<h4>Proficient</h4>

				<% if(profarea.length > 0){ %>

                                    	<% for (var i = 0 ; i < profarea.length-1 ; i++){ %>

                                          <a href="#" class="btn btn-success" title="">
                                                <span class="text-sm text-gray">
                                                    <%= profarea[i]%>
                                                 </span>
                                             </a>
                                      <% } %>
                                      <% } %>


		</div>


	</div>
	<br><br><br>

<h1> RECOMMENDATION SYSTEM </h1>
	<div class="tags">
		<div class="box1">
			<h4>Easy</h4>

		  <% if(easyc.length > 0){ %>

                                              	<% for (var i = 0 ; i < easyc.length-1 ; i++){ %>

                                                    <a href=<%= easyl[i]%> class="btn btn-danger" title="">
                                                          <span class="text-sm text-gray">
                                                              <%= easyc[i]%>
                                                           </span>
                                                       </a>
                                                <% } %>
                                                <% } %>

		</div>


		<div class="box2">
			<h4>Medium</h4>

			  <% if(mediumc.length > 0){ %>

                                                          	<% for (var i = 0 ; i < mediumc.length-1 ; i++){ %>

                                                                <a href=<%= mediuml[i]%> class="btn btn-warning" title="">
                                                                      <span class="text-sm text-gray">
                                                                          <%= mediumc[i]%>
                                                                       </span>
                                                                   </a>
                                                            <% } %>
                                                            <% } %>

		</div>


		<div class="box3">
			<h4>Hard</h4>

			 <% if(hardc.length > 0){ %>

                                                                      	<% for (var i = 0 ; i < hardc.length-1 ; i++){ %>

                                                                            <a href=<%= hardl[i]%> class="btn btn-success" title="">
                                                                                  <span class="text-sm text-gray">
                                                                                      <%= hardc[i]%>
                                                                                   </span>
                                                                               </a>
                                                                        <% } %>
                                                                        <% } %>

		</div>


	</div>
